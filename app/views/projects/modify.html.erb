<%= render 'article/nav'%>
<h1>Modify Projects</h1>
<% @projects.each do |r|%>
    <li>
      <%= r.name %>
      <%= link_to '[E]', projects_edt_path+'?'+'edt='+r.id.to_s%>
      <%= link_to '[X]', projects_modify_path+'?'+'del='+r.id.to_s%>
      <%= link_to '[Add status]', projects_add_status_path+'?'+'add='+r.id.to_s%>
      <%= link_to '[Add members]', projects_add_members_path+'?project_id='+r.id.to_s%>
    </li>
<% end%>