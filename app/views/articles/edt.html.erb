<%= render 'article/nav'%>
<h1>Edit Article</h1>

<%= form_for :articles_edt, url: :articles_edt_handle do |f| %>
    <p>
      <%= f.label  :title %><br>
      <%= f.text_field  :title, :value => @article.title %>
    </p>

    <p>
      <%= f.label  :text %><br>
      <%= f.text_area  :text, :value => @article.content %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>